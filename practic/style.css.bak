body {
    width: 400px;
    padding: 0;
    margin: auto auto;
}
table {
    border-collapse: collapse;
}
td {
    width: 50px;
    height: 50px;
}
.bl {
    background-color: blue;
}
.rd {
    background-color: red;
}
.td-one {
    border-radius: 25px 25px 0 0;
}
.td-seven {
    border-radius: 0 0 25px 25px;
}
.td-nine {
    border-radius: 0 25px 25px 0;
}
.td-eight {
    border-radius: 0 0 0 25px;
}
.td-two {
    border-radius: 25px 0 0 0;
}
.td-three {
    border-radius: 0 25px 25px 0;
}
.td-six {
    border-radius: 50%;
}
